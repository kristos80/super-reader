{
  "$schema": "https://raw.githubusercontent.com/infection/infection/0.28.1/resources/schema.json",
  "source": {
    "directories": [
      "src"
    ],
  },
  "mutators": {
    "@default": true,
    "global-ignoreSourceCodeByRegex": [
      "(?s).*data = array_merge\\([^;]*;"
    ]
  },
  "logs": {
    "text": "infection.log",
    "stryker": {
      "badge": "master"
    },
    github: true
  }
}
