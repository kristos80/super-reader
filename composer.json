{
  "name": "kristos80/super-reader",
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "marcocesarato/php-conventional-changelog": "^1",
    "pestphp/pest": "^2",
    "phpmd/phpmd": "^2",
    "vlucas/phpdotenv": "^5"
  },
  "autoload": {
    "psr-4": {
      "Kristos80\\SuperReader\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kristos80\\SuperReader\\Tests\\": "tests"
    }
  },
  "version": "0.0.1",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  }
}
